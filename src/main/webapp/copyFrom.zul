<?page title="new page title" contentType="text/html;charset=UTF-8"?>
<zk>
	<window id="copyWin" title="Copy From..." width="800px" height="600px" border="normal" apply="org.zkoss.bind.BindComposer" viewModel="@id('vm')@init('com.cht.iTest.vm.CopyFromViewModel')">
		<style>
   .z-tree-icon .z-tree-open {
     background-image: url(data:image/png;base64,);
     background-size: cover;
     width: 100%;
   }
   .z-tree-icon .z-tree-close {
     background-image: url(data:image/png;base64,);
     background-size: cover;
     width: 100%;
   }
   /*remove the font awesome icon*/
   .z-tree-icon .z-tree-open::before,
   .z-tree-icon .z-tree-close::before {
     content: '';
   }
  </style>
		<vlayout hflex="1" vflex="1">
			<hlayout hflex="1" vflex="1">
				<groupbox closable="false" mold="3d" hflex="1" vflex="1">
					<caption label="@load(vm.tar.name)" />
					<tree model="@bind(vm.tarTreeModel)" hflex="1" vflex="1" style="border:none;">
						<template name="model">
							<treeitem >
								<treerow>
									<treecell>${each.data.name}</treecell>
								</treerow>
							</treeitem>
						</template>
					</tree>
				</groupbox>
				<groupbox closable="false" hflex="1" mold="3d" vflex="1">
					<caption label="複製來源-">
						<listbox mold="select" model="@bind(vm.testPlans)" hflex="1" onSelect="@command('selectPlan')" />
					</caption>
					<tree model="@bind(vm.srcTreeModel)" hflex="1" vflex="1" style="border:none;">
						<template name="model">
							<treeitem >
								<treerow >
									<treecell>${each.data.name}</treecell>
								</treerow>
							</treeitem>
						</template>
					</tree>
				</groupbox>
			</hlayout>
			<toolbar align="center">
				<button label="確定" image="/imgs/ok.png" onClick="@command('confirm')" />
				<button label="放棄" image="/imgs/cancel.png" onClick="copyWin.detach();" />
			</toolbar>
		</vlayout>
	</window>
</zk>